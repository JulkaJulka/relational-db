CREATE TABLE SUPPLIERS(
SUPPLIER_ID NUMBER,
CONSTRAINT SUPPLIER_PK PRIMARY KEY(SUPPLIER_ID),
COMPANY_NAME NVARCHAR2(50),
CONTACT_NAME VARCHAR(20),
CONTACT_TITLE NVARCHAR2(50),
ADDRESS NVARCHAR2(50),
CITY VARCHAR(20),
REGION NVARCHAR2(50),
POSTAL_CODE NUMBER,
COUNTRY VARCHAR(20),
PHONE NVARCHAR2(10),
FAX NVARCHAR2(10),
HOME_PAGE NVARCHAR2(50)
);
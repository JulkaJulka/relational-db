CREATE TABLE CUSTOMERS(
CUSTOMER_ID NUMBER,
CONSTRAINT CUSTOMER_PK PRIMARY KEY(CUSTOMER_ID),
COMPANY_NAME NVARCHAR2(50),
CONTACT_NAME VARCHAR(20),
CONTACT_TITLE VARCHAR(20),
ADDRESS  NVARCHAR2(50),
CITY VARCHAR(20),
REGION VARCHAR(20),
POSTAL_CODE NUMBER,
COUNTRY VARCHAR(20),
PHONE NVARCHAR2(15),
FAX NVARCHAR2(15)
);